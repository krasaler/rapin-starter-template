{
  "name": "rapin-starter-template",
  "version": "0.0.1",
  "description": "Rapin Starter Template",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "serve": "cross-env NODE_ENV=production node lib/index.js",
    "start": "npm run serve",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "cross-env NODE_ENV=development nodemon lib/index.js",
    "watch-ts": "tsc -w"
  },
  "author": "krasaler",
  "license": "ISC",
  "dependencies": {
    "@rapin/inky": "^0.0.2",
    "@rapin/typeorm": "^1.0.4",
    "@rapin/typeorm-auth": "^1.0.2",
    "cross-env": "^7.0.2",
    "rapin": "rapinjs/rapin#dev",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "nodemon": "^1.19.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.14.2"
  }
}
